# sumę wszystkich liczb parzystych od 2 do 100 (włącznie).
zad1=0
for x in range(2,101):
    if(x%2==0):
        zad1 +=x
print(zad1)

# sumę kwadratów wszystkich liczb od 1 do 100 (włącznie).
zad2=0
for x in range(1,101):
    zad2+=x*x
print(zad2)

# sumę potęg liczby 2 dla wykładników od 1 do 63 (włącznie).
zad3=0
for x in range(1,64):
    zad3+=2**x
print(zad3)

